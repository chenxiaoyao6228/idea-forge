    "iconv-lite": "^0.6.3",
    "jsx-email": "^2.2.0",
    "lodash": "^4.17.21",
    "nest-winston": "^1.9.7",
    "nestjs-trpc": "^1.6.1",
    "nestjs-zod": "^4.2.0",
    "nodemailer": "^6.9.15",
    "openai": "^4.78.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "5.11.0",
    "prisma-extension-pagination": "^0.7.4",
    "react": "^18.3.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
