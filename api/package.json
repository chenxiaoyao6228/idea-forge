{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "predev": "chmod +x scripts/start-docker.sh && ./scripts/start-docker.sh",
    "dev": "npm run dev:docker && npm run prisma:dev && nodemon",
    "dev:docker": "sh scripts/start-docker.sh",
    "start:debug": "NODE_ENV=development dotenvx run --  nest start --debug 9333 --watch --preserveWatchOutput",
    "dev:studio": "concurrently \"npm run start:debug\" \"npm run prisma:studio\"",
    "email:dev": "email preview ./src/_shared/email/templates",
    "build": "npm run prisma:generate && NODE_ENV=production webpack --config webpack.config.js",
    "start:prod": "NODE_ENV=production dotenvx run -- node dist/main",
    "start:pm2": "dotenvx run -- pm2 start ecosystem.config.js --env production --no-daemon",
    "start": "NODE_ENV=production dotenvx run -- nest start",
    "pm2:logs": "pm2 logs",
    "pm2:monitor": "pm2 monit",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "lint": "biome lint .",
    "lint:fix": "biome check --apply .",
    "format": "biome format . ",
    "format:fix": "biome format --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "setup": "chmod +x scripts/start-docker.sh && chmod +x ./scripts/setup.sh  && ./scripts/setup.sh",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:studio": " npx prisma studio --port 5656",
    "prisma:migrate:local": "npx prisma migrate dev",
    "prisma:init:local": "npx prisma migrate deploy &&  npx prisma db seed",
    "prisma:reset:local": "npx prisma migrate reset &&  npx prisma db seed",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/s3-request-presigner": "^3.717.0",
    "@dotenvx/dotenvx": "^1.36.0",
    "@faker-js/faker": "8.4.1",
    "@hocuspocus/extension-database": "^2.13.5",
