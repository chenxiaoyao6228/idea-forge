{
  "name": "nest-react-vite-starter",
  "version": "1.0.0",
  "description": "Nest.js + React + Vite Starter",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd api && npm run dev",
    "dev:client": "cd client && npm dev",
    "build": "npm run build:client && npm run build:api",
    "build:client": "cd client && npm run build",
    "build:api": "cd api && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "YorkChan",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}